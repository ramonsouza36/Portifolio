@model IEnumerable <Dominio.Entidades.ItensVendas>
<table style="width:80%" id="cabecalhoProd">
    <thead>
        <tr id="cabeçalho">
            <th>Código</th>
            <th>Valor unitário</th>
            <th>Quantidade</th>
            <th>Valor Total</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.CodigoVenda</td>
                <td>@item.Item</td>
                <td>@item.ValorUnitario</td>
                <td>@item.Quantidade</td>
            </tr>
        }
    </tbody>
</table>